-  
-  # Test should return error response for headers exceeding limit
-  run curl -s -w "%{http_code}" -H "X-Custom-Header: $large_header" -d "test-payload" 127.0.0.1:5000/ -o /dev/null
-  [ "$status" -eq 0 ]
