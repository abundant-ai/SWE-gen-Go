-type dbStatsCollector struct {
-	db *sql.DB
-
-	maxOpenConnections *prometheus.Desc
-
-	openConnections  *prometheus.Desc
-	inUseConnections *prometheus.Desc
-	idleConnections  *prometheus.Desc
-
